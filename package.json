{
  "name": "shiikai-extensions",
  "version": "1.0.0",
  "description": "Paperback/Felii extensions for Spanish manga sites",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:source": "webpack --config webpack.config.js --env entry=./0.8/MangaTV/index.js --env filename=source.js",
    "build:index": "webpack --config webpack.config.js --env entry=./0.8/MangaTV/index.js --env filename=index.js"
  },
  "keywords": [
    "paperback",
    "extensions",
    "manga"
  ],
  "author": "WiLyDTG",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@paperback/toolchain": "^1.0.0-alpha.57",
    "@types/node": "^25.2.3",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "browserify-shim": "^3.8.16",
    "typescript": "^5.9.3",
    "webpack": "^5.105.2",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@paperback/types": "^1.0.0-alpha.57",
    "cheerio": "^1.0.0-rc.12"
  }
}
